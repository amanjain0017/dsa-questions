class Solution{
    public:
    int setSetBit(int x, int y, int l, int r){
        // code here
        if(l<1 or r>32)     -1;
        
        for(int i = l; i<=r; i++){
            
            int mask = 1<<(i-1);
            
            if(mask & y)
                x = x | mask;
        }
        
        return x;
    }
};